{
  "name": "new-typeorm-project",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/cookie": "^0.4.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.9",
    "@types/express-fileupload": "^1.1.6",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.10",
    "@types/nodemailer": "^6.4.0",
    "concurrently": "^5.3.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/cors": "^2.8.9",
    "@types/slug": "^0.9.1",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-fileupload": "^1.2.1",
    "jsonwebtoken": "^8.5.1",
    "nodemailer": "^6.5.0",
    "paginator": "^1.0.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.10",
    "slug": "^4.0.2",
    "typeorm": "0.2.29",
    "typeorm-seeding": "^1.6.1"
  },
  "scripts": {
    "start":"node build/server.js",
    "server": "nodemon --exec ts-node src/server.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "add-migration": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "update-database": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "client": "cd ../client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client \" -n server,client  ",
    "migrate": "typeorm migration:generate -n"
  }
}
